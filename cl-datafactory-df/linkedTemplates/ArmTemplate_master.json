{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "cl-datafactory-df"
		},
		"AdfByExample_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AdfByExample'"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureKeyVaultLS_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://demo-project-kv-kv.vault.azure.net/"
		},
		"GetSalesData_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "storage-access-key"
		},
		"storageDataSetLS_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "storage-access-key"
		},
		"storageDataSourceLS_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "storage-access-key"
		},
		"storageDestinationLS_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "storage-access-key"
		},
		"storageLS_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "storage-access-key"
		},
		"AzureBlobStorageJolly_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "storage-access-key"
		},
		"AzureDataLakeStore1_properties_typeProperties_dataLakeStoreUri": {
			"type": "string",
			"defaultValue": "https://undefined.azuredatalakestore.net/webhdfs/v1"
		},
		"AzureDataLakeStore1_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "72f988bf-86f1-41af-91ab-2d7cd011db47"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfdemoteck.dfs.core.windows.net/"
		},
		"AzureBlobStorageJollyGood_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "storage-access-key"
		},
		"linkedServiceExchangeRate_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "storage-access-key"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "cl-datafactory-df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AdfByExample_connectionString": {
						"value": "[parameters('AdfByExample_connectionString')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureKeyVaultLS_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVaultLS_properties_typeProperties_baseUrl')]"
					},
					"AzureDataLakeStore1_properties_typeProperties_dataLakeStoreUri": {
						"value": "[parameters('AzureDataLakeStore1_properties_typeProperties_dataLakeStoreUri')]"
					},
					"AzureDataLakeStore1_properties_typeProperties_tenant": {
						"value": "[parameters('AzureDataLakeStore1_properties_typeProperties_tenant')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"GetSalesData_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('GetSalesData_properties_typeProperties_connectionString_secretName')]"
					},
					"storageDataSetLS_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('storageDataSetLS_properties_typeProperties_connectionString_secretName')]"
					},
					"storageDataSourceLS_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('storageDataSourceLS_properties_typeProperties_connectionString_secretName')]"
					},
					"storageDestinationLS_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('storageDestinationLS_properties_typeProperties_connectionString_secretName')]"
					},
					"storageLS_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('storageLS_properties_typeProperties_connectionString_secretName')]"
					},
					"AzureBlobStorageJolly_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('AzureBlobStorageJolly_properties_typeProperties_connectionString_secretName')]"
					},
					"AzureBlobStorageJollyGood_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('AzureBlobStorageJollyGood_properties_typeProperties_connectionString_secretName')]"
					},
					"linkedServiceExchangeRate_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('linkedServiceExchangeRate_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "cl-datafactory-df_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'cl-datafactory-df_ArmTemplate_0')]"
			]
		}
	]
}